{
  "name": "rapid-step-10",
  "version": "1.0.0",
  "description": "Parked personal website",
  "scripts": {
    "build": "npx @11ty/eleventy && npx tailwindcss -i ./css/index.css -o _site/css/index.css",
    "ci:build": "npx @11ty/eleventy && npx tailwindcss -i ./css/index.css -o _site/css/index.css",
    "bench": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
    "serve": "npx @11ty/eleventy --serve --watch",
    "tailwind": "npx tailwindcss -i ./css/index.css -o ./_site/css/index.css --watch",
    "debug": "DEBUG=* npx @11ty/eleventy",
    "refresh-data": "node scripts/refreshSpreadsheetData.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:AnalyzePlatypus/rapid-step-10.git"
  },
  "author": {
    "name": "Michoel Samuels",
    "email": "michoel+rapidstep10@hey.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/AnalyzePlatypus/rapid-step-10",
  "dependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "dotenv": "^16.0.3",
    "eleventy-plugin-i18n": "^0.1.3",
    "eleventy-plugin-pwa": "^1.0.8",
    "eleventy-plugin-svg-contents": "^0.7.0",
    "eleventy-plugin-tailwindcss": "^0.3.0",
    "google-spreadsheet": "^4.0.0",
    "luxon": "^2.3.1",
    "markdown-it": "^12.3.2",
    "markdown-it-anchor": "^8.4.1",
    "papaparse": "^5.4.1",
    "razorux-eleventy-tools": "^0.0.3",
    "svg-country-flags": "^1.2.10"
  },
  "devDependencies": {
    "tailwindcss": "^3.1"
  }
}
